{
  "name": "project_oreo",
  "version": "1.0.0",
  "description": "project_oreo is a online fashion store made with mern stack",
  "main": "index.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "client-install": "npm install --prefix ./client",
    "admin-install": "npm install --prefix ./admin",
    "admin": "npm start --prefix ./admin",
    "client": "npm start --prefix ./client",
    "server": "nodemon index.js",
    "start": "node index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "locus": "^2.0.4",
    "mongoose": "^6.9.2",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "save-dev": "0.0.1-security",
    "validator": "^13.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lizardKingLK/project_oreo"
  },
  "keywords": [
    "project_oreo",
    "group_oreo",
    "mern stack",
    "node",
    "express",
    "react"
  ],
  "author": "group_oreo",
  "license": "ISC"
}
